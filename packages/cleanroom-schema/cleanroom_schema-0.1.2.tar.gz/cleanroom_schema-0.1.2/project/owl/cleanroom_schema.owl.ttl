@prefix BFO: <https://purl.obolibrary.org/obo/BFO_> .
@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix OBI: <https://purl.obolibrary.org/obo/OBI_> .
@prefix cleanroom_schema: <https://example.com/biosample_relations/cleanroom-schema/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cleanroom_schema:MaterialSample a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "MaterialSample" ;
    dcterms:title "Material Sample" ;
    rdfs:subClassOf cleanroom_schema:Specimen ;
    skos:exactMatch OBI:0000747 .

cleanroom_schema:type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "type" ;
    rdfs:range linkml:String ;
    skos:definition "The type of a thing. Should be a local class name or an ontology class from a local enumeration" ;
    skos:note "be conservative about assigning rdf:type as the slot uri",
        "try designates_type (but wouldn't work for enumerated ontology classes)" .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

<https://w3id.org/microbiomedata/cleanroom-schema> a owl:Ontology ;
    rdfs:label "cleanroom-schema" ;
    IAO:0000700 cleanroom_schema:DataListCollection,
        cleanroom_schema:NamedThing,
        cleanroom_schema:ReifiedRelationship ;
    dcterms:license "MIT" ;
    dcterms:title "cleanroom-schema" ;
    rdfs:seeAlso "https://github.com/microbiomedata/nmdc-schema/issues/518",
        "https://microbiomedata.github.io/cleanroom-schema" ;
    linkml:generation_date "2022-12-07T19:37:35" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "cleanroom_schema.yaml" ;
    linkml:source_file_date "2022-12-07T19:36:54" ;
    linkml:source_file_size 6370 .

cleanroom_schema:DataListCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DataListCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom cleanroom_schema:CollectingBiosamplesFromSite ;
            owl:onProperty cleanroom_schema:cbfs_list ],
        [ a owl:Restriction ;
            owl:allValuesFrom cleanroom_schema:Biosample ;
            owl:onProperty cleanroom_schema:biosample_list ],
        [ a owl:Restriction ;
            owl:allValuesFrom cleanroom_schema:FieldResearchSite ;
            owl:onProperty cleanroom_schema:frs_list ],
        [ a owl:Restriction ;
            owl:allValuesFrom cleanroom_schema:ReifiedRelationship ;
            owl:onProperty cleanroom_schema:relations_list ] ;
    skos:definition "A datastructure that can contain lists of instances from any selected classes in the schema" .

cleanroom_schema:ImmaterialEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ImmaterialEntity" ;
    dcterms:title "Immaterial Entity" ;
    rdfs:subClassOf cleanroom_schema:NamedThing ;
    skos:exactMatch BFO:0000141 .

cleanroom_schema:MaterialEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "MaterialEntity" ;
    dcterms:title "Material Entity" ;
    rdfs:subClassOf cleanroom_schema:NamedThing ;
    skos:exactMatch BFO:0000040 .

cleanroom_schema:PlannedProcess a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "PlannedProcess" ;
    dcterms:title "Planned Process" ;
    rdfs:subClassOf cleanroom_schema:NamedThing ;
    skos:exactMatch OBI:0000011 .

cleanroom_schema:Site a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Site" ;
    dcterms:title "Site" ;
    rdfs:subClassOf cleanroom_schema:ImmaterialEntity ;
    skos:exactMatch BFO:0000029 .

cleanroom_schema:Specimen a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Specimen" ;
    dcterms:title "Specimen" ;
    rdfs:subClassOf cleanroom_schema:MaterialEntity ;
    skos:exactMatch OBI:0100051 .

cleanroom_schema:biosample_list a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "biosample_list" ;
    rdfs:range cleanroom_schema:Biosample .

cleanroom_schema:biosamples a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "biosamples" ;
    rdfs:range cleanroom_schema:Biosample ;
    skos:definition "A list of Biosamples" ;
    skos:note "for example, the biosamples that wre collected at some site" .

cleanroom_schema:cbfs_list a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "cbfs_list" ;
    rdfs:range cleanroom_schema:CollectingBiosamplesFromSite .

cleanroom_schema:collected_from a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "collected_from" ;
    rdfs:domain cleanroom_schema:Biosample ;
    rdfs:range cleanroom_schema:FieldResearchSite ;
    skos:definition "The Site from which a Biosample was collected" ;
    skos:note "this illustrates implementing a Biosample relation with a (binary) slot" .

cleanroom_schema:frs_list a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "frs_list" ;
    rdfs:range cleanroom_schema:FieldResearchSite .

cleanroom_schema:notes a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "notes" ;
    rdfs:range linkml:String ;
    skos:note "this is a placeholder for more structured metadata about a relationship... by whom? when? status?" .

cleanroom_schema:relations_list a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "relations_list" ;
    rdfs:range cleanroom_schema:ReifiedRelationship .

cleanroom_schema:site a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "site" ;
    rdfs:range cleanroom_schema:FieldResearchSite ;
    skos:definition "a single site" ;
    skos:note "could this ever be something other than a FieldResearchSite?",
        "for example, the site from which a biosample was collected" .

rdf:object a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "object" ;
    rdfs:range cleanroom_schema:NamedThing ;
    skos:definition "The object of a relationship" ;
    skos:exactMatch rdf:object ;
    skos:note "could this be multivalued?" .

rdf:predicate a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "predicate" ;
    rdfs:range linkml:String ;
    skos:definition "The predicate of a relationship" ;
    skos:exactMatch rdf:predicate ;
    skos:note "what's the range here? we would expect the name of a local slot, or the CURIE representation of an object property, preferably from the OBO foundry, skos, schema.org, PROV, etc" .

rdf:subject a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subject" ;
    rdfs:range cleanroom_schema:NamedThing ;
    skos:definition "The subject of a relationship" ;
    skos:exactMatch rdf:subject ;
    skos:note "could this be multivalued?" .

cleanroom_schema:CollectingBiosamplesFromSite a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CollectingBiosamplesFromSite" ;
    dcterms:title "Planned Process" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom cleanroom_schema:Biosample ;
            owl:onProperty cleanroom_schema:biosamples ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass cleanroom_schema:FieldResearchSite ;
            owl:onProperty cleanroom_schema:site ],
        cleanroom_schema:PlannedProcess ;
    skos:exactMatch OBI:0000011 ;
    skos:note "not to be confused with a reified relationship class",
        "this illustrates implementing a Biosample relation with a process class" .

schema:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable description for a thing" ;
    skos:exactMatch schema:description .

schema:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:exactMatch schema:identifier .

schema:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable name for a thing" ;
    skos:exactMatch schema:name .

cleanroom_schema:ReifiedRelationship a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ReifiedRelationship" ;
    rdfs:seeAlso "https://stackoverflow.com/questions/45610092/owl-reification-vs-rdf-reification" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdf:predicate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass cleanroom_schema:NamedThing ;
            owl:onProperty rdf:object ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass cleanroom_schema:NamedThing ;
            owl:onProperty rdf:subject ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty cleanroom_schema:notes ] ;
    skos:exactMatch rdf:Statement .

cleanroom_schema:Biosample a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Biosample" ;
    dcterms:title "Biosample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass cleanroom_schema:FieldResearchSite ;
            owl:onProperty cleanroom_schema:collected_from ],
        cleanroom_schema:NamedThing .

cleanroom_schema:FieldResearchSite a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "FieldResearchSite" ;
    dcterms:title "Field Research Site" ;
    rdfs:subClassOf cleanroom_schema:Site ;
    skos:altLabel "research plot" ;
    skos:definition "A site, outside of a laboratory, from which biosamples may be collected." ;
    skos:note "In Bioscales, one might say that rhizosphere soil and bulk soil from around the same plant were obtained at the same site. In this case, the site would correspond to one tree.",
        "Provides grouping of biosamples at level that's more specific than belonging to the same study",
        "be very clear that this could be part of a larger site",
        "might correspond to GOLD's identifier an a page like https://gold.jgi.doe.gov/biosample?id=Gb0305833" .

cleanroom_schema:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:description ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema:Thing ;
    skos:note "class_uri: BFO:0000001?" .

linkml:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" .

linkml:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" .


